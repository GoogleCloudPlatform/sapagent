          },
          "value": "AGENT_NAME"
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "agent_version"
          },
          "value": "AGENT_VERSION"
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "network_ips"
          },
          "value": "NETWORK_IPS"
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "collection_config_version",
            "min_version": "2.8"
          },
          "value": "COLLECTION_CONFIG_VERSION"
        }
      ],
      "os_command_metrics": [
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "gcloud"
          },
          "os_vendor": "ALL",
          "command": "gcloud",
          "args": ["-v"],
          "and_eval_rules": {
            "eval_rules": [
              {
                "output_source": "STDOUT",
                "output_contains": "Google Cloud SDK"
              }
            ],
            "if_true": {
              "value_from_literal": "true"
            },
            "if_false": {
              "value_from_literal": "false"
            }
          }
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "gsutil"
          },
          "os_vendor": "ALL",
          "command": "gsutil",
          "args": ["-v"],
          "and_eval_rules": {
            "eval_rules": [
              {
                "output_source": "STDOUT",
                "output_contains": "gsutil version"
              }
            ],
            "if_true": {
              "value_from_literal": "true"
            },
            "if_false": {
              "value_from_literal": "false"
            }
          }
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "agent_state"
          },
          "os_vendor": "ALL",
          "command": "systemctl",
          "args": ["is-active", "google-cloud-sap-agent"],
          "or_eval_rules": {
            "or_eval_rules": [
              {
                "eval_rules": [
                  {
                    "output_source": "STDOUT",
                    "output_contains": "could not be found"
                  }
                ],
                "if_true": {
                  "value_from_literal": "notinstalled"
                }
              },
              {
                "eval_rules": [
                  {
                    "output_source": "STDOUT",
                    "output_equals": "active"
                  }
                ],
                "if_true": {
                  "value_from_literal": "running"
                },
                "if_false": {
                  "value_from_literal": "notrunning"
                }
              }
            ]
          }
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "os_settings",
            "min_version": "3.2"
          },
          "os_vendor": "ALL",
          "command": "/usr/bin/google_cloud_sap_agent",
          "args": ["configureinstance", "-check"],
          "or_eval_rules": {
            "or_eval_rules": [
              {
                "eval_rules": [
                  {
                    "output_source": "EXIT_CODE",
                    "output_equals": "0"
                  }
                ],
                "if_true": {
                  "value_from_literal": "pass"
                }
              },
              {
                "eval_rules": [
                  {
                    "output_source": "EXIT_CODE",
                    "output_equals": "1"
                  }
                ],
                "if_true": {
                  "value_from_literal": "fail"
                },
                "if_false": {
                  "value_from_literal": ""
                }
              }
            ]
          }
        }
      ]
    },
