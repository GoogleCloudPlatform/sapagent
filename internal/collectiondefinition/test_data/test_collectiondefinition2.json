  "workload_validation": {
    "version": 4,
    "validation_system": {
      "system_metrics": [
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "instance_name"
          },
          "value": "INSTANCE_NAME"
        },
        {
          "metric_info": {
            "type": "workload.googleapis.com/sap/validation/system",
            "label": "os"
          },
